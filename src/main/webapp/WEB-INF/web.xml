<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Controller</servlet-name>
  	<display-name>Controller</display-name>
  	<description></description>
  	<servlet-class>controller.Controller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterController</servlet-name>
  	<display-name>RegisterController</display-name>
  	<description></description>
  	<servlet-class>controller.RegisterController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetData</servlet-name>
  	<display-name>GetData</display-name>
  	<description></description>
  	<servlet-class>controller.GetData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetDatabase</servlet-name>
  	<display-name>GetDatabase</display-name>
  	<description></description>
  	<servlet-class>controller.GetDatabase</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Controller</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterController</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetData</servlet-name>
  	<url-pattern>/getdata</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetDatabase</servlet-name>
  	<url-pattern>/displaydatabase</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  		<welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
